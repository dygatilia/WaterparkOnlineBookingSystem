<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>UserController</servlet-name>
        <servlet-class>CONTROLLER.UserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaffController</servlet-name>
        <servlet-class>CONTROLLER.StaffController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MessageServlet</servlet-name>
        <servlet-class>CONTROLLER.MessageController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeaturesController</servlet-name>
        <servlet-class>CONTROLLER.FeaturesController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RulesController</servlet-name>
        <servlet-class>CONTROLLER.RulesController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TicketsController</servlet-name>
        <servlet-class>CONTROLLER.TicketsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FacilitiesController</servlet-name>
        <servlet-class>CONTROLLER.FacilitiesController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PromotionsController</servlet-name>
        <servlet-class>CONTROLLER.PromotionsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingController</servlet-name>
        <servlet-class>CONTROLLER.BookingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingStaffController</servlet-name>
        <servlet-class>CONTROLLER.BookingStaffController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardController</servlet-name>
        <servlet-class>CONTROLLER.DashboardController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserController</servlet-name>
        <url-pattern>/UserController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaffController</servlet-name>
        <url-pattern>/StaffController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MessageServlet</servlet-name>
        <url-pattern>/MessageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeaturesController</servlet-name>
        <url-pattern>/FeaturesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RulesController</servlet-name>
        <url-pattern>/RulesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TicketsController</servlet-name>
        <url-pattern>/TicketsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FacilitiesController</servlet-name>
        <url-pattern>/FacilitiesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PromotionsController</servlet-name>
        <url-pattern>/PromotionsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingController</servlet-name>
        <url-pattern>/BookingController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingStaffController</servlet-name>
        <url-pattern>/BookingStaffController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashboardController</servlet-name>
        <url-pattern>/DashboardController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
